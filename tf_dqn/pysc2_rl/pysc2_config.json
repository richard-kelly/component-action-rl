{
    "use_batch": true,
    "batch_file": "batch.json",
    "batch_runs": 20,
    "copy_model_from": "",
    "inference_only": false,
    "inference_only_epsilon": 0,

    "model_dir": "models/5m_nov_22",
    "model_checkpoint_frequency": 100000,
    "model_checkpoint_max": 1,
    "model_checkpoint_every_n_hours": 24,
    "max_steps": 250000,
    "max_episodes": 0,

    "learning_rate": 1e-3,
    "learning_rate_decay_method": "none",
    "learning_rate_decay_steps": 10000,
    "learning_rate_decay_param": 1.0,

    "double_DQN": true,
    "dueling_network": true,
    "bootstrapping_steps": 1,
    "discount": 0.99,
    "batch_size": 64,
    "reg_type": "l2",
    "reg_scale": 1e-3,
    "use_component_weights": false,

    "update_frequency": 4,
    "memory_size": 60000,
    "memory_burn_in": 10000,
    "target_update_frequency": 10000,

    "use_priority_experience_replay": true,
    "per_alpha": 0.6,
    "per_starting_beta": 0.4,
    "per_beta_anneal_steps": 250000,

    "initial_epsilon": 1.0,
    "final_epsilon": 0.1,
    "decay_steps": 150000,
    "decay_type": "linear",

    "debug_logging": false,
    "log_frequency": 200,

    "env": {
        "screen_size": 84,
        "minimap_size": 84,
        "map_name": "combat_5m_LTD2_on_damage_norm_factor_1000",
        "visualize": false,
        "step_mul": 8,
        "action_functions": [
            {
                "type": "include_range",
                "list": [0, 548]
            },
            {
                "reason": "exclude 500 long queue ids and select ids",
                "type": "exclude_list",
                "list": [5, 10]
            },
            {
                "reason": "exclude control groups because they are not very useful without an RNN",
                "type": "exclude_list",
                "list": [4]
            },
            {
                "reason": "exclude minimap actions",
                "type": "exclude_list",
                "list": [1, 13, 15, 17, 20, 547, 543, 544, 545, 542, 332, 334, 336, 338, 340, 342, 344, 346, 348, 350, 452, 517, 519, 521, 523]
            }
        ],
        "use_screen2": true,
        "use_queue": false,
        "use_select_add": false,
        "select_rect_half_size": 20,
        "use_all_unit_types": false,
        "use_specific_unit_types": false,
        "specific_unit_types": ["Marine", "Stalker"]
    }
}