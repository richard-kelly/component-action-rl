{
    "use_batch": true,
    "batch_file": "batch.json",
    "batch_runs": 20,
    "copy_model_from": "",
    "inference_only": false,
    "inference_only_realtime": false,
    "inference_only_epsilon": 0,
    "inference_only_episodes": 100,

    "use_scripted_bot": "",

    "model_dir": "models/test18",
    "model_checkpoint_frequency": 100000,
    "model_checkpoint_max": 1,
    "model_checkpoint_every_n_hours": 24,
    "max_steps": 250000,
    "max_episodes": 0,

    "step_penalty": 0,
    "episode_extra_win_reward": 0,

    "learning_rate": 1e-3,
    "learning_rate_decay_method": "none",
    "learning_rate_decay_steps": 10000,
    "learning_rate_decay_param": 1.0,

    "double_DQN": true,
    "dueling_network": true,
    "bootstrapping_steps": 1,
    "discount": 0.99,
    "batch_size": 64,
    "reg_type": "l2",
    "reg_scale": 1e-3,

    "do_eval_episodes": true,
    "train_on_eval_episodes": false,
    "one_eval_episode_per": 10,

    "loss_formula": "avg_y_compared_to_components",
    "loss_formula_choices": [
        "avg_y_compared_to_components",
        "avg_y_compared_to_avg_prediction",
        "pairwise_component_comparison"
    ],

    "update_frequency": 4,
    "memory_size": 60000,
    "memory_burn_in": 10000,
    "target_update_frequency": 10000,

    "use_priority_experience_replay": true,
    "per_alpha": 0.6,
    "per_starting_beta": 0.4,
    "per_beta_anneal_steps": 250000,
    "match_per_beta_anneal_steps_to_max": true,
    "match_per_beta_anneal_steps_ratio": 1.0,
    "avg_component_tds": true,

    "initial_epsilon": 1.0,
    "final_epsilon": 0.1,
    "decay_steps": 150000,
    "decay_type": "linear",
    "match_epsilon_decay_steps_to_max": true,
    "match_epsilon_decay_steps_ratio": 0.75,

    "debug_logging": false,
    "log_frequency": 200,

    "network_structure": {
        "shared_spatial_network": [
            {"type": "conv", "filters": 16, "kernel_size": 7},
            {"type": "conv", "filters": 32, "kernel_size": 5},
            {"type": "conv", "filters": 64, "kernel_size": 3}
        ],
        "value_network": [
            {"type": "avg_pool", "pool_size": 3, "strides": 3, "padding": "valid"},
            "flatten",
            256,
            256
        ],
        "shared_non_spatial_network": [
            {"type": "avg_pool", "pool_size": 3, "strides": 3, "padding": "valid"},
            "flatten",
            256,
            256
        ],
        "component_stream_default": [],
        "component_stream_specs": {
            "function": [],
            "screen": [{"type": "conv", "filters": 1, "kernel_size": 1}],
            "screen2": [{"type": "conv", "filters": 1, "kernel_size": 1}]
        },
        "end_spatial_streams_with_dense_instead_of_flatten": false,
        "use_stream_outputs_as_inputs_to_other_streams": false,
        "stream_dependencies": {
            "screen": ["function"],
            "screen2": ["screen"]
        },
        "default_activation": "relu",
        "scale_gradients_at_shared_spatial_split": true,
        "scale_gradients_at_shared_non_spatial_split": true,
        "use_histograms": true
    },

    "env": {
        "screen_size": 84,
        "minimap_size": 84,
        "map_name": "combat_5m_LTD2_on_damage_norm_factor_1000",
        "visualize": false,
        "step_mul": 8,
        "action_functions": [
            {
                "reason": "no_op, select_rect, select_army, attack_screen, move_screen",
                "type": "include_list",
                "list": [0, 3, 7, 12, 331]
            }
        ],
        "use_screen2": true,
        "use_queue": false,
        "use_select_add": false,
        "select_rect_half_size": 20,
        "use_all_unit_types": false,
        "use_specific_unit_types": false,
        "specific_unit_types": ["Marine", "Stalker"],

        "use_hp_log_values": false,
        "use_shield_log_values": false,
        "use_hp_ratios": false,
        "use_shield_ratios": false,
        "use_hp_cats": true,
        "hp_cats_values": [15, 30, 50, 100],
        "use_shield_cats": true,
        "shield_cats_values": [15, 30, 50, 100]
    }
}
