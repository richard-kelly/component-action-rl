{
    "type": "ppo_agent",

    "update_mode": {
        "unit": "episodes",
        "batch_size": 2,
        "frequency": 2
    },
    "memory": {
        "type": "latest",
        "include_next_states": false,
        "capacity": 2000
    },

    "step_optimizer": {
        "type": "adam",
        "learning_rate": 1e-3
    },
    "subsampling_fraction": 0.1,
    "optimization_steps": 50,

    "discount": 0.99,
    "entropy_regularization": 0.01,
    "gae_lambda": null,
    "likelihood_ratio_clipping": 0.2,

    "execution": {
        "type": "single",
        "session_config": null,
        "distributed_spec": null
    },

    "actions_exploration": {
        "type": "epsilon_anneal",
        "initial_epsilon": 0.9,
        "final_epsilon": 0.1,
        "timesteps": 3000000
    }
}
